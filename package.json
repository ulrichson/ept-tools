{
  "name": "ept-tools",
  "version": "0.0.13",
  "description": "Entwine Point Tiles utility library",
  "repository": "github:connormanning/ept-tools",
  "main": "lib/index.js",
  "bin": {
    "ept": "lib/app/index.js"
  },
  "scripts": {
    "build": "ttsc -p tsconfig.build.json && copyfiles -f src/lib/* lib/lib/ && ncc build lib/app/index.js --minify --out dist && mv dist/index.js dist/ept",
    "dev": "ts-node -r tsconfig-paths/register src/app/index.ts",
    "prepublish": "$npm_execpath run build",
    "test": "jest"
  },
  "dependencies": {
    "ajv": "^7.1.1",
    "fatproj": "^0.0.2",
    "forager": "^0.0.6",
    "fs-extra": "^9.1.0",
    "log-symbols": "^4.0.0",
    "protopath": "^0.0.0",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.7",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@types/yargs": "^16.0.1",
    "@vercel/ncc": "^0.38.1",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "copyfiles": "^2.4.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "ts-jest": "^26.5.2",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.2.2"
  }
}
